<%- include('partials/header'); %>
    <form action="/compose" method="post">
        <h1>Compose</h1>
        <div class="form-group">
            <label >Title</label>
            <input type="text" class="form-control" placeholder="Enter Title" name="postTitle" autocomplete="off">
        </div>

        <div class="form-group">
            <label for="exampleFormControlTextarea1">Post</label>
            <textarea class="form-control" id="exampleFormControlTextarea1" rows="5" name="postBody" placeholder="Write Your Post" autocomplete="off"></textarea>
        </div>

        <button type="submit" class="btn btn-primary">Publish</button>
    </form>
<%- include('partials/footer'); %>